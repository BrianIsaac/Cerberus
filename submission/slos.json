{
  "slos": [
    {
      "name": "AI Agents - Availability SLO",
      "description": "99.9% of requests across all AI agents should complete successfully without errors",
      "type": "metric",
      "thresholds": [
        {
          "target": 99.9,
          "timeframe": "30d",
          "warning": 99.95
        },
        {
          "target": 99.5,
          "timeframe": "7d",
          "warning": 99.7
        }
      ],
      "query": {
        "numerator": "sum:trace.http.request.hits{service:ops-assistant}.as_count() - sum:trace.http.request.errors{service:ops-assistant}.as_count()",
        "denominator": "sum:trace.http.request.hits{service:ops-assistant}.as_count()"
      },
      "tags": [
        "team:ai-agents",
        "slo_type:availability"
      ],
      "monitor_ids": []
    },
    {
      "name": "AI Agents - Latency SLO",
      "description": "95% of the time, P95 latency across all AI agents should be under 10 seconds",
      "type": "monitor",
      "thresholds": [
        {
          "target": 95.0,
          "timeframe": "30d",
          "warning": 97.0
        },
        {
          "target": 90.0,
          "timeframe": "7d",
          "warning": 92.0
        }
      ],
      "tags": [
        "team:ai-agents",
        "slo_type:latency"
      ],
      "monitor_ids": [11218408]
    },
    {
      "name": "AI Agents - Governance SLO",
      "description": "99% of requests across all AI agents should stay within step budgets and have no tool failures",
      "type": "metric",
      "thresholds": [
        {
          "target": 99.0,
          "timeframe": "30d",
          "warning": 99.5
        },
        {
          "target": 95.0,
          "timeframe": "7d",
          "warning": 97.0
        }
      ],
      "query": {
        "numerator": "sum:trace.http.request.hits{service:ops-assistant}.as_count() - sum:ai_agent.step_budget_exceeded{service:ops-assistant}.as_count() - sum:ai_agent.tool.errors{service:ops-assistant}.as_count()",
        "denominator": "sum:trace.http.request.hits{service:ops-assistant}.as_count()"
      },
      "tags": [
        "team:ai-agents",
        "slo_type:governance"
      ],
      "monitor_ids": []
    },
    {
      "name": "AI Agents - Quality SLO (Hallucination-Free)",
      "description": "99.5% of requests across all AI agents should not produce hallucinated responses",
      "type": "metric",
      "thresholds": [
        {
          "target": 99.5,
          "timeframe": "30d",
          "warning": 99.7
        },
        {
          "target": 98.0,
          "timeframe": "7d",
          "warning": 99.0
        }
      ],
      "query": {
        "numerator": "sum:llmobs.evaluation.hallucination.total{service:ops-assistant}.as_count() - sum:llmobs.evaluation.hallucination.fail{service:ops-assistant}.as_count()",
        "denominator": "sum:llmobs.evaluation.hallucination.total{service:ops-assistant}.as_count()"
      },
      "tags": [
        "team:ai-agents",
        "slo_type:quality",
        "evaluation:hallucination"
      ],
      "monitor_ids": []
    }
  ],
  "slo_metadata": {
    "created_by": "ai-agents-fleet-deployment",
    "purpose": "Track fleet-wide health, performance, governance, and LLM quality metrics across all AI agents tagged with team:ai-agents",
    "alert_configuration": {
      "availability_slo": {
        "error_budget_alerts": [
          {
            "threshold": 50,
            "message": "50% of availability error budget consumed. Review recent errors and incidents across AI agent fleet."
          },
          {
            "threshold": 90,
            "message": "90% of availability error budget consumed. CRITICAL - immediate action required for AI agent fleet."
          }
        ]
      },
      "latency_slo": {
        "error_budget_alerts": [
          {
            "threshold": 50,
            "message": "50% of latency error budget consumed. Review slow traces across AI agent fleet."
          },
          {
            "threshold": 90,
            "message": "90% of latency error budget consumed. CRITICAL - performance degradation in AI agent fleet."
          }
        ]
      },
      "governance_slo": {
        "error_budget_alerts": [
          {
            "threshold": 50,
            "message": "50% of governance error budget consumed. Review agent behaviour and tool reliability across fleet."
          },
          {
            "threshold": 90,
            "message": "90% of governance error budget consumed. CRITICAL - agent reliability issues in fleet."
          }
        ]
      },
      "quality_slo": {
        "error_budget_alerts": [
          {
            "threshold": 25,
            "message": "25% of quality error budget consumed. Review hallucination rates across AI agent fleet."
          },
          {
            "threshold": 75,
            "message": "75% of quality error budget consumed. CRITICAL - LLM quality degradation in AI agent fleet."
          }
        ]
      }
    }
  }
}
